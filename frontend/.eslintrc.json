{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
	"no-unused-vars": 0,
	"no-undef": 0,
	"no-console": 0,
	"no-shadow": 0,
	"max-len":0
    }
}